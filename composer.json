  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AllinGo\\NotificationsClient\\": "src/"
    }
  },
  "require": {
    "php": ">=8.2",
    "guzzlehttp/guzzle": "^7.8|^8.0",
    "illuminate/support": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AllinGo\\NotificationsClient\\NotificationsClientServiceProvider"
      ]
    }
  }
}
